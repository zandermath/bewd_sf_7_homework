<div class="container-fluid">
<div class="col-md-6 col-md-offset-3 form-container-devise square">
<ul class="list-unstyled">
<% @users.each do |user| %>
	<li> <%= user.name %></li>
	<%= simple_form_for(user, url: registration_path(user), html: { method: :put}) do |f| %>
		<%= f.input :admin, as: :boolean, checked_value: true, unchecked_value: false %>
		<%= f.button :submit %>
	<% end %>
<% end %>
</ul>
</div>
</div>
